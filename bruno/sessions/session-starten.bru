meta {
  name: Session Starten
  type: http
  seq: 1
}

post {
  url: {{baseUrl}}/api/sessions
  body: json
  auth: bearer
}

auth:bearer {
  token: {{childToken}}
}

body:json {
  {
    "child_id": {{childId}},
    "type": "switch",
    "coins": 1
  }
}

script:post-response {
  if (res.status === 200) {
    bru.setVar("sessionId", res.body.id);
  }
}
