meta {
  name: TV Freigeben
  type: http
  seq: 4
}

patch {
  url: {{mikrotikUrl}}/rest/ip/firewall/address-list/{{mikrotikEntryId}}
  body: json
  auth: basic
}

auth:basic {
  username: {{mikrotikUser}}
  password: {{mikrotikPass}}
}

body:json {
  {
    "disabled": "true"
  }
}

docs {
  disabled=true bedeutet: der Eintrag ist deaktiviert → IP greift nicht mehr in der Sperrliste → TV freigegeben.
}
